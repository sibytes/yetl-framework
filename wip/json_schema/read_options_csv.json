{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/read_options_csv.schema.json",
  "title": "Delimited Text Dataset",
  "description": "An delimited text format described by a flat file format. CSV is a misnomer however it's consistent with spark",
  "type": "object",
  "properties": {
    "schema_versioning": {
      "type": "boolean"
    },
    "column_delimiter": {
      "type": "string"
    },
    "row_delimiter": {
      "type": "string"
    },
    "skip_line_count": {
      "type": "integer",
      "minimum": 0
    },
    "column_names_ifr": {
      "type": "boolean"
    },
    "quote_character": {
      "type": "string"
    },
    "null_value": {
      "type": "string"
    },
    "empty_column_null": {
      "type": "boolean"
    },
    "timestamp_format": {
      "type": "string"
    },
    "date_format": {
      "type": "string"
    }
  },
  "required": [
    "schema_versioning",
    "column_delimiter",
    "row_delimiter",
    "skip_line_count",
    "column_names_ifr",
    "escape_character",
    "quote_character",
    "null_value",
    "empty_column_null",
    "timestamp_format",
    "date_format"
  ]
}
